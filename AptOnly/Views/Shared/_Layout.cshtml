<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ApartmentFS</title>

        <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/site.css" />

        <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
        <script src="~/lib/jquery/jquery-3.3.1.min.js"></script>
        <script src="~/lib/popper/popper.js"></script>

        <script src="~/js/site.js" asp-append-version="true"></script>


</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">ApartmentFS</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div id="navbarNavDropdown" class="navbar-collapse collapse">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item active">
                    <a asp-area="" asp-controller="Home" asp-action="Index" class="nav-link">Home</a>
                </li>

            </ul>
            <ul class="navbar-nav">

                <partial name="_LoginPartial" />
            </ul>
        </div>
    </nav>


    <!--Here starts the listing part of the apartments-->
    <!--Here ends the listing part of the apartments-->
    <partial name="_CookieConsentPartial" />


        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2019 - ApartmentFS</p>
        </footer>



    @RenderSection("Scripts", required: false)
</body>
</html>
